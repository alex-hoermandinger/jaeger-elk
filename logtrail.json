{
  "version" : 1,
  "index_patterns" : [
    {
      "es": {
        "default_index": "mma-*"
      },
      "tail_interval_in_seconds": 10,
      "es_index_time_offset_in_seconds": 0,
      "display_timezone": "local",
      "display_timestamp_format": "MMM DD HH:mm:ss",
      "max_buckets": 500,
      "default_time_range_in_days" : 30,
      "max_hosts": 100,
      "max_events_to_keep_in_viewer": 10000,
      "fields" : {
        "mapping" : {
            "timestamp" : "@timestamp",
            "hostname" : "host",
            "program": "loggerName",
            "message": "message"
        },
        "message_format": "{{{message}}}"
      },
      "color_mapping" : {
      }
    }
  ]
}

